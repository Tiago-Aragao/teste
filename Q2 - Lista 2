#include <stdio.h>

int main() {
  int idade;
  printf("Digite uma idade: ");
  scanf("%d", &idade);
  if (idade < 0) {
    printf("Digite uma idade VALIDA!\n");
  }
  else if (idade >= 60){
    printf("Idoso\n");
  }
  else if (idade < 60 && idade >= 18) {
    printf("Adulto\n");
  }
  else if (idade < 18 && idade >= 13) {
    printf("Adolescente\n");
  }
  else {
    printf("Crian√ßa\n");
  }
   return 0;
}
