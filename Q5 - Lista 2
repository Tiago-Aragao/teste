#include <stdio.h>
int main() {
  // Declaração de Variavel:
  int numero = 100, limite = 300;
  // Inicio do laço de repetição:
  while(numero < limite) {
    // Incremento da variavel:
    numero++;
    // Mostra apenas os numeros impares:
    if (numero % 2 != 0) {
      // Se for o ultimo numero, não mostra a virgula e sim ponto final:
      if (numero == limite-1) {
        printf("%d.", numero);
      }
      // Se não for, mostra a virgula:
      else {
        printf("%d, ", numero);
      }    
    }
  }
  return 0; // Fim do Programa.
}
